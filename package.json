{
  "name": "shrikar-debouncer",
  "version": "1.0.0",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "type": "module",
  "description": "CommonJS optimized debounce utility.",
  "keywords": [
    "debounce utility"
  ],
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build:cjs": "esbuild index.js --bundle --format=cjs --outfile=dist/index.cjs",
    "build:esm": "esbuild index.js --bundle --format=esm --outfile=dist/index.mjs",
    "build": "npm run build:cjs && npm run build:esm"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "esbuild": "^0.27.2",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.5",
    "prettier": "^3.8.1",
    "tsup": "^8.5.1"
  }
}

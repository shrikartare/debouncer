{
  "name": "shrikar-debouncer",
  "version": "1.0.0",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "type": "module",
  "description": "CommonJS optimized debounce utility.",
  "keywords": [
    "debounce utility"
  ],
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build:cjs": "esbuild src/index.js --bundle --format=cjs --outfile=dist/index.cjs",
    "build:esm": "esbuild src/index.js --bundle --format=esm --outfile=dist/index.mjs",
    "build": "npm run build:cjs && npm run build:esm",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepare": "husky",
    "lint-staged": "lint-staged",
    "test": "jest"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "esbuild": "^0.27.2",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jest": "^29.12.1",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "jest-esbuild": "^0.4.0",
    "lint-staged": "^16.2.7",
    "prettier": "^3.8.1"
  },
  "lint-staged": {
    "**/*.{js,jsx}": [
      "eslint"
    ]
  }
}
